<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_able_to_change_Inactive_to_Active</fullName>
    <active>false</active>
    <errorConditionFormula>AND( ISCHANGED(Loyalty_Card_Status__c), ISPICKVAL(PRIORVALUE(Loyalty_Card_Status__c),&quot;Inactive&quot;))</errorConditionFormula>
    <errorDisplayField>CurrencyIsoCode</errorDisplayField>
    <errorMessage>Cannot change Inactive to Active</errorMessage>
</ValidationRule>
