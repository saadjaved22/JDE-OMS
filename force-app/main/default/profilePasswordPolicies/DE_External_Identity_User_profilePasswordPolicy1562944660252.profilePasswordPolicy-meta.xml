<?xml version="1.0" encoding="UTF-8" ?>
<ProfilePasswordPolicy xmlns="http://soap.sforce.com/2006/04/metadata">
    <forgotPasswordRedirect>true</forgotPasswordRedirect>
    <lockoutInterval>0</lockoutInterval>
    <maxLoginAttempts>5</maxLoginAttempts>
    <minimumPasswordLength>8</minimumPasswordLength>
    <minimumPasswordLifetime>false</minimumPasswordLifetime>
    <obscure>false</obscure>
    <passwordComplexity>2</passwordComplexity>
    <passwordExpiration>180</passwordExpiration>
    <passwordHistory>24</passwordHistory>
    <passwordQuestion>1</passwordQuestion>
    <profile>DE External Identity User</profile>
</ProfilePasswordPolicy>
